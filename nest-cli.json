{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/timory/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/timory/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/timory",
  "projects": {
    "timory": {
      "type": "application",
      "root": "apps/timory",
      "entryFile": "main",
      "sourceRoot": "apps/timory/src",
      "compilerOptions": {
        "tsConfigPath": "apps/timory/tsconfig.app.json"
      }
    },
    "timory-batch": {
      "type": "application",
      "root": "apps/timory-batch",
      "entryFile": "main",
      "sourceRoot": "apps/timory-batch/src",
      "compilerOptions": {
        "tsConfigPath": "apps/timory-batch/tsconfig.app.json"
      }
    }
  }
}